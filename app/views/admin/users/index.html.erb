
<table class="table table-hover">
  <thead>
    <tr>

      <th>First Name</th>
      <th>Last Name</th>
      <th>Email</th>
      <th>
        Nomadlist username
      </th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
    <tr>
      <td><%= user.first_name %></td>
      <td><%= user.last_name %></td>
      <td><%= user.email %></td>
      <td><%= user.nomadlist_username %></td>
    </tr>
    <% end %>
  </tbody>
</table>
