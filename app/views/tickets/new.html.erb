<div class="row">
  <h1>Upload document</h1>
</div>
<hr>
<div class="row">
  <%= simple_form_for [@trip, @transportation, @ticket] do |f|  %>
    <%= f.filepicker_field :filepicker_url, button_text: "Upload", button_class:"btn btn-outline-primary", services: "COMPUTER, GMAIL, GOOGLE_DRIVE, DROPBOX" %>
    <%= f.submit "Validate", class:"btn btn-primary" %>
    <% end %>
</div>
